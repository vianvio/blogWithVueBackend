{
  "name": "resume",
  "plural": "resumes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "AddUserInfo": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "birthday": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "wechat": {
      "type": "string"
    },
    "qq": {
      "type": "string"
    },
    "selfAssignment": {
      "type": "string",
      "dataType": "text"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "ownerId"
    },
    "lastUpdatedBy": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "lastUpdatedById"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "methods": {}
}
