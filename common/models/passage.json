{
  "name": "passage",
  "plural": "passages",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "AddUserInfo": true
  },
  "properties": {
    "content": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "ownerId"
    },
    "lastUpdatedBy": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "lastUpdatedById"
    },
    "passageTypeId": {
      "type": "belongsTo",
      "model": "passageType",
      "foreignKey": "passageTypeId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
