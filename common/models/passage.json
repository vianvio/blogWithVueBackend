{
  "name": "passage",
  "plural": "passages",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "AddUserInfo": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "string",
      "dataType": "text"
    },
    "tag": {
      "type": "string"
    },
    "bDisabled": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "ownerId"
    },
    "lastUpdatedBy": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": "lastUpdatedById"
    },
    "passageType": {
      "type": "belongsTo",
      "model": "passageType",
      "foreignKey": "passageTypeId"
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }, {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }, {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    }, {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "findById"
    }

  ],
  "methods": {}
}
